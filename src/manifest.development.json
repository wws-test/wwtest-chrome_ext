{
    "manifest_version": 3,
    "name": "Trun快速测试",
    "description": "快速测试工具",
    "icons": {
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "version": "0.0.1",

    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "*://*/*"
    ],


    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "/content.js"

            ]
        }
    ]
}